{
  "name" : "RJSLibUF",
  "options" : {
    "deploymentTarget" : "11.0",
    "bundleIdPrefix" : "com.rjps.libuf",
    "findCarthageFrameworks" : false,
    "xcodeVersion" : "12.0.1",
    "minimumXcodeGenVersion" : "2.18.0",
    "generateEmptyDirectories" : true,
    "groupSortPosition" : "top"
  },
  "configs" : {
    "Release" : "release",
    "Debug" : "debug"
  },
  "aggregateTargets" : {
    "SwiftLint" : {
      "buildScripts" : [
        {
          "script" : "${SRCROOT}\/Scripts\/swiftlint.sh\n",
          "shell" : "\/bin\/bash",
          "name" : "SwiftLint"
        }
      ]
    }
  },
  "settings" : {
    "configs" : {
      "Release" : {
        "ENABLE_BITCODE" : true
      }
    },
    "base" : {
      "SWIFT_VERSION" : 5.2999999999999998,
      "IPHONEOS_DEPLOYMENT_TARGET" : 13,
      "ENABLE_BITCODE" : false
    }
  },
  "targets" : {
    "RJSLibUFAppThemes" : {
      "templates" : [
        "Framework"
      ],
      "sources" : [
        {
          "path" : "..\/Sources\/RJSLibUFAppThemes"
        }
      ],
      "dependencies" : [
        {
          "target" : "RJSLibUFBase",
          "link" : false
        }
      ]
    },
    "SampleApp" : {
      "sources" : [
        {
          "path" : "..\/SampleApp"
        }
      ],
      "settings" : {
        "base" : {
          "MARKETING_VERSION" : 1,
          "OTHER_SWIFT_FLAGS" : "-DINCLUDE_VIP_TEMPLATE"
        }
      },
      "platform" : "iOS",
      "dependencies" : [
        {
          "target" : "RJSLibUFBase",
          "link" : true
        },
        {
          "link" : true,
          "target" : "RJSLibUFAppThemes"
        },
        {
          "target" : "RJSLibUFNetworking",
          "link" : true
        },
        {
          "link" : true,
          "target" : "RJSLibUFStorage"
        },
        {
          "target" : "RJSLibUFBaseVIP",
          "link" : true
        },
        {
          "target" : "RJSLibUFDesignables",
          "link" : true
        }
      ],
      "type" : "application",
      "deploymentTarget" : 13
    },
    "RJSLibUFDesignables" : {
      "sources" : [
        {
          "path" : "..\/Sources\/RJSLibUFDesignables"
        }
      ],
      "dependencies" : [
        {
          "target" : "RJSLibUFBase",
          "link" : false
        },
        {
          "target" : "RJSLibUFAppThemes",
          "link" : false
        }
      ],
      "templates" : [
        "Framework"
      ]
    },
    "RJSLibUFBase" : {
      "sources" : [
        {
          "path" : "..\/Sources\/RJSLibUFBase"
        }
      ],
      "templates" : [
        "Framework"
      ]
    },
    "RJSLibUFNetworking" : {
      "dependencies" : [
        {
          "target" : "RJSLibUFBase",
          "link" : false
        }
      ],
      "templates" : [
        "Framework"
      ],
      "sources" : [
        {
          "path" : "..\/Sources\/RJSLibUFNetworking"
        }
      ]
    },
    "RJSLibUFStorage" : {
      "dependencies" : [
        {
          "link" : false,
          "target" : "RJSLibUFBase"
        }
      ],
      "sources" : [
        {
          "path" : "..\/Sources\/RJSLibUFStorage"
        }
      ],
      "templates" : [
        "Framework"
      ]
    },
    "RJSLibUFBaseVIP" : {
      "dependencies" : [
        {
          "target" : "RJSLibUFBase",
          "link" : false
        },
        {
          "target" : "RJSLibUFDesignables",
          "link" : false
        }
      ],
      "sources" : [
        {
          "path" : "..\/Sources\/RJSLibUFBaseVIP"
        }
      ],
      "templates" : [
        "Framework"
      ]
    },
    "MyLibraryTests" : {
      "type" : "bundle.unit-test",
      "scheme" : {

      },
      "dependencies" : [
        {
          "link" : false,
          "target" : "SampleApp"
        },
        {
          "link" : false,
          "target" : "RJSLibUFBase"
        },
        {
          "target" : "RJSLibUFNetworking",
          "link" : false
        },
        {
          "target" : "RJSLibUFStorage",
          "link" : false
        }
      ],
      "sources" : [
        {
          "path" : "..\/Tests\/RJSLibUFTests"
        }
      ],
      "platform" : "iOS"
    }
  },
  "include" : [
    "_FrameworkTemplate.yml",
    "_SwiftLint.yml"
  ],
  "targetTemplates" : {
    "Framework" : {
      "type" : "framework",
      "deploymentTarget" : "13.0",
      "platform" : "iOS"
    }
  }
}